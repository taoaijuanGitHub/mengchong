.container {
   position: fixed;
   top:0;
   left:0;
   right:0;
   bottom:80rpx;
}
.container map{
  width:100%;
  height:100%;
}
.btns {
  width:100%;
  position: fixed;
  bottom:0;
  display: flex;
  line-height: 80rpx;
  background: #ff9700;
  color:white;
}
.btns view{
   flex:1;
   text-align: center;
}
.btns view:nth-of-type(2){
  background: #03bbd5;
}