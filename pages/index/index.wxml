<!--index.wxml-->
<view class="container">
  <map id="map" longitude="{{longitude}}" latitude="{{latitude}}" scale="16" show-location controls="{{controls}}" markers="{{markers}}" bindcontroltap="controltap" bindmarkertap="markertap"></map>
</view>
<view class="btns">
  <view bindtap="go" data-page="/pages/publish/publish">发布</view>
  <view  bindtap="go" data-page="/pages/search/search">搜索</view>
</view>